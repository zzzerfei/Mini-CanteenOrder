.server {
  width: 100%;
  background: #fff;
  position: relative;
  top: 50rpx;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  justify-content: space-between;
}

.server-item {
  margin-left: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}

.server-item text {
  vertical-align: middle;
  margin-left: 16rpx;
}
